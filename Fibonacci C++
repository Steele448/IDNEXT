#include <iostream>

using namespace std;

int fibonacci(int n) {
    if (n == 0){
        return 0;
    }
    if (n == 1){
        return 1;
    }
    else{
    return fibonacci(n-1)+fibonacci(n-2);
    // Complete the function.
    }
}

int main() {
    int n;
    cout<<"Enter a number for the term in a Fibonacci Series: "<<endl;
    cin >> n;
    cout << fibonacci(n);
    return 0;
}
